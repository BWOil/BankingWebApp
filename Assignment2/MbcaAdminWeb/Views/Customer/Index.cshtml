<h3>All customer</h3>


<h2>Customer List</h2>

<table>
    <tr>
        <th>ID</th>
        <th>Name</th>

    </tr>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@customer.CustomerID</td>
            <td>@customer.Name</td>
            Other columns as needed
        </tr>
    }
</table>


}